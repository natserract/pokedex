{
  "name": "pokedex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run env:dev -- next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "env:dev": "npx dotenv -e .env/.development.env",
    "e2e": "npm run env:dev -- start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "npm run env:dev -- start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "npm run env:dev -- cypress open --component",
    "component:headless": "npm run env:dev -- cypress run --component"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "axios": "^1.6.8",
    "axios-cache-interceptor": "^1.5.1",
    "clsx": "^2.1.0",
    "lucide-react": "^0.363.0",
    "next": "14.1.4",
    "nprogress": "^0.2.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cypress": "13.6.2",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "local-cypress": "1.2.6",
    "postcss": "^8",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
